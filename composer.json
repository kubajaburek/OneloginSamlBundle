{
    "name": "hslavich/oneloginsaml-bundle",
    "type": "symfony-bundle",
    "description": "OneLogin SAML Bundle for Symfony2",
    "keywords": ["onelogin", "saml", "sso"],
    "license": "MIT",
    "authors": [
        {
            "name": "hslavich",
            "email": "hernan.slavich@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~4.4.0|^5.1",
        "symfony/security-bundle": "~4.4.0|^5.1",
        "onelogin/php-saml": "^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~4.4.0|^5.1",
        "doctrine/orm": "^2",
        "php-coveralls/php-coveralls": "^2",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": { "Hslavich\\OneloginSamlBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
